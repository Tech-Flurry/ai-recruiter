  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "skipLibCheck": true,
    "module": "ESNext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}